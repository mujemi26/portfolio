/* Custom styles for animations and enhanced UI - Refined Glassmorphic (Slicker) */
        :root {
            --bg: #f7f8fa;
            --text: #181c23;
            --card-bg: #fff;
            --card-border: #e5e7eb;
            --accent: #3b5bdb;
            --subtitle: #3b4252;
            --cta-bg: #fff;
            --cta-text: #181c23;
            --cta-border: #3b5bdb;
            --icon: #3b4252;
            --icon-hover: #3b5bdb;
        }
        [data-theme="dark"] {
            --bg: #16181d;
            --text: #f7f8fa;
            --card-bg: #23272f;
            --card-border: #232b3b;
            --accent: #7ed6df;
            --subtitle: #b0b8c1;
            --cta-bg: #23272f;
            --cta-text: #f7f8fa;
            --cta-border: #7ed6df;
            --icon: #b0b8c1;
            --icon-hover: #7ed6df;
        }
        body, html {
            font-family: 'Inter', 'Segoe UI', 'Roboto', 'Helvetica Neue', Arial, sans-serif;
            font-size: 1.08rem;
            font-weight: 400;
            letter-spacing: 0.01em;
            background: var(--bg);
            background-size: 200% 200%;
            animation: gradientBG 10s ease-in-out infinite;
            min-height: 100vh;
            color: var(--text);
            line-height: 1.7;
            transition: background 0.4s, color 0.4s;
        }
        @keyframes gradientBG {
            0% {background-position: 0% 50%;}
            50% {background-position: 100% 50%;}
            100% {background-position: 0% 50%;}
        }
        .container {
            max-width: 1000px; /* Slightly wider container */
            margin: 0 auto;
            padding: 2rem;
        }
        .section {
            margin-bottom: 4rem; /* Increased section spacing */
            padding: 1px 2rem; /* Added horizontal padding to sections */
            max-width: 70%; /* Changed max-width to 70% */
            margin-left: auto; /* Center the section horizontally */
            margin-right: auto;
            transition: transform 0.3s ease; /* Added transition for scaling */
            border-radius: 1rem;
            background: var(--card-bg);
            box-shadow: 0 8px 32px 0 rgba(106,130,251,0.18);
            backdrop-filter: blur(10px);
            border: 1.5px solid var(--card-border);
            color: var(--text);
            transition: background 0.4s, color 0.4s, border 0.4s;
            font-weight: 500;
        }
        .section:hover {
            transform: translateY(-6px) scale(1.03);
            box-shadow: 0 16px 48px 0 rgba(106,130,251,0.22);
        }
        .section:not(:hover) {
             transform: scale(1);
             box-shadow: 0 4px 8px rgba(0, 0, 0, 0.15);
        }

        .section-title, #education .section-title, [data-theme='light'] .section-title, [data-theme='dark'] .section-title {
            font-size: 2.25rem;
            font-weight: 700;
            margin-bottom: 1.5rem;
            text-align: center;
            text-shadow: 0 3px 6px rgba(0, 0, 0, 0.4);
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 0.5rem;
        }
        .section-title i {
            font-size: 1.75rem;
            margin-right: 0.5rem;
            color: #a7f3d0;
            text-shadow: 0 1px 2px rgba(0,0,0,0.3);
        }
        .hero-section {
            background: var(--bg) !important;
            color: var(--text) !important;
            min-height: 60vh;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            padding-top: 8.5rem;
            padding-bottom: 4rem;
        }
        .hero-section > *:not(.css-wave-bg) {
            position: relative;
            z-index: 2;
        }
        .css-wave-bg {
            position: absolute;
            top: 0; left: 0; width: 100%; height: 120px;
            z-index: 0;
            pointer-events: none;
            background: url('data:image/svg+xml;utf8,<svg width="100%25" height="120" viewBox="0 0 1440 320" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill="%236a82fb" fill-opacity="0.3" d="M0,160L80,170C160,180,320,200,480,192C640,184,800,144,960,133.3C1120,123,1280,149,1360,162.7L1440,176L1440,320L1360,320C1280,320,1120,320,960,320C800,320,640,320,480,320C320,320,160,320,80,320L0,320Z"></path></svg>');
            background-size: 200% 100%;
            animation: waveMove 8s ease-in-out infinite alternate;
        }
        @keyframes waveMove {
            0% { background-position-x: 0%; }
            100% { background-position-x: 100%; }
        }
        @media (max-width: 600px) {
            html, body {
                overflow-x: hidden;
                max-width: 100vw;
            }
            .container, .section {
                max-width: 100vw !important;
                width: 100vw !important;
                padding: 0 0.5rem !important;
                margin: 0 !important;
                box-sizing: border-box;
            }
            .section {
                font-size: 1.1rem;
                border-radius: 0.7rem;
                margin-bottom: 2rem !important;
            }
            .hero-section {
                padding: 3.5rem 0.5rem 1.5rem 0.5rem !important;
                min-height: 50vh;
                border-radius: 0 0 1.2rem 1.2rem;
            }
            .hero-title {
                font-size: 1.5rem !important;
                margin-top: 1.2rem;
                margin-bottom: 0.7rem;
            }
            .hero-subtitle {
                font-size: 1rem !important;
                margin-bottom: 1.2rem;
            }
            .hero-section .hero-buttons {
                flex-direction: column;
                gap: 0.7rem;
                margin-bottom: 1.5rem;
                width: 100%;
            }
            .contact-button, .cv-download-button {
                width: 100%;
                justify-content: center;
                padding: 1rem 0.5rem;
                font-size: 1.05rem;
                margin: 0;
            }
            .skills-grid {
                grid-template-columns: 1fr !important;
                gap: 1rem !important;
                padding-bottom: 1.2rem !important;
            }
            .skill-card {
                padding: 1.1rem 0.7rem !important;
                min-height: 0 !important;
                border-radius: 0.7rem !important;
                font-size: 1rem;
            }
            .skill-title {
                font-size: 1.1rem !important;
                margin-bottom: 0.5rem;
            }
            .skill-details-btn {
                font-size: 0.98rem;
                padding: 0.4rem 1rem;
            }
            .about-me {
                padding: 0 0.5rem;
            }
            .about-content {
                font-size: 0.98rem;
                max-width: 100%;
                margin-bottom: 1.2rem;
            }
            .experience-card, .project-card, .education-card {
                padding: 1.1rem 0.7rem 1.1rem 2.2rem !important;
                border-radius: 0.7rem !important;
                margin-bottom: 1.2rem !important;
                font-size: 1rem;
            }
            .experience-icon, .project-icon, .education-icon {
                left: 0.7rem !important;
                top: 1.1rem !important;
                width: 1.7rem !important;
                height: 1.7rem !important;
                font-size: 1rem !important;
            }
            .footer {
                padding: 1.2rem 0.5rem !important;
                font-size: 0.85rem !important;
                margin-top: 2rem !important;
            }
            .section-title {
                font-size: 1.2rem !important;
                margin-bottom: 1rem !important;
                gap: 0.3rem !important;
            }
        }
        @media (max-width: 400px) {
            .hero-title {
                font-size: 1.1rem !important;
            }
            .section-title {
                font-size: 1rem !important;
            }
        }
        .hero-accent-dot {
            display: inline-block;
            width: 10px;
            height: 10px;
            border-radius: 50%;
            background: linear-gradient(135deg, #5b7fff, #7ed6df);
            margin-bottom: 1.5rem;
            box-shadow: 0 2px 8px rgba(91,127,255,0.10);
        }
        .hero-title {
            font-size: 2.6rem;
            font-weight: 900;
            color: var(--text);
            text-shadow: 0 2px 8px rgba(0,0,0,0.04);
            letter-spacing: -0.01em;
            text-align: center;
            line-height: 1.15;
            margin-bottom: 1.1rem;
            background: none;
            text-shadow: none;
            margin-top: 2.2rem;
        }
        .hero-underline {
            display: block;
            width: 80px;
            height: 6px;
            margin: 0.5rem auto 1.5rem auto;
            border-radius: 3px;
            background: linear-gradient(90deg, #6a82fb, #fc5c7d);
        }
        .hero-subtitle {
            font-size: 1.18rem;
            font-weight: 500;
            color: var(--subtitle) !important;
            text-shadow: 0 1px 4px rgba(0,0,0,0.03);
            text-align: center;
            margin-bottom: 2.1rem;
            letter-spacing: 0.01em;
        }
        .hero-section .hero-buttons {
            display: flex;
            flex-direction: row;
            align-items: center;
            justify-content: center;
            gap: 1.1rem;
            margin-bottom: 2.5rem;
        }
        .cta-button {
            display: inline-flex;
            align-items: center;
            justify-content: center;
            padding: 1rem 2.5rem; /* Slightly increased button padding */
            font-size: 1.25rem; /* Larger button text (text-lg) */
            font-weight: 600; /* Use 600 for Inter's semi-bold (font-semibold) */
            color: #fff;
            background: var(--cta-bg) !important;
            border-radius: 0.75rem; /* More rounded button (rounded-lg) */
            transition: transform 0.2s cubic-bezier(.4,2,.6,1), box-shadow 0.2s, background 0.3s;
            box-shadow: 0 6px 24px rgba(252,92,125,0.15);
            cursor: pointer;
            border: 2px solid var(--cta-border);
            outline: none;
            gap: 0.5rem;
        }
        .cta-button:hover {
            transform: translateY(-6px) scale(1.08) rotate(-2deg);
            box-shadow: 0 12px 32px rgba(252,92,125,0.18);
            background: var(--cta-border) !important;
        }
        .cta-button:active {
            transform: translateY(0) scale(0.95);
            box-shadow: 0 3px 6px rgba(0, 0, 0, 0.2); /* Smaller active shadow */
        }
        .about-me {
            display: flex;
            flex-direction: column;
            align-items: center;
            text-align: center;
            padding: 0 1rem;
        }
        .about-content {
            font-size: 1.1rem; /* Slightly larger body text (text-base) */
            color: #e5e7eb; /* Gray-200, even lighter text */
            line-height: 1.9; /* Slightly increased about me line height */
            margin-bottom: 2rem; /* Increased about me spacing */
            max-width: 90%; /* Increased max-width for about me */
            text-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
        }
        .skills-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 2rem;
            margin-top: 1rem;
            padding-bottom: 2.5rem;
        }
        .skill-card {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: flex-start;
            position: relative;
            background-color: rgba(255, 255, 255, 0.06); /* Slightly more transparent skill cards */
