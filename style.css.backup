/* Floating Theme Toggle for Mobile */
.floating-theme-toggle {
  display: none;
  position: fixed;
  left: 2.2rem;
  bottom: 2.2rem;
  z-index: 1200;
  background: linear-gradient(135deg,#3b5bdb,#16a085);
  color: #fff;
  border: none;
  border-radius: 50%;
  width: 48px;
  height: 48px;
  box-shadow: 0 4px 16px rgba(59,91,219,0.18);
  cursor: pointer;
  transition: opacity 0.35s cubic-bezier(.4,0,.2,1), transform 0.35s cubic-bezier(.4,0,.2,1), background 0.3s;
            align-items: center;
  justify-content: center;
  outline: none;
  font-size: 1.5rem;
  opacity: 0;
  pointer-events: none;
  transform: scale(0.85);
}

.floating-theme-toggle.visible {
  opacity: 1;
  pointer-events: auto;
  transform: scale(1);
}

.floating-theme-toggle:hover {
  background: linear-gradient(135deg,#16a085,#3b5bdb);
  transform: scale(1.12);
  box-shadow: 0 8px 32px 0 #16a08544;
}

.floating-theme-toggle.visible:hover {
  transform: scale(1.12);
}

.floating-theme-toggle i {
  transition: transform 0.3s ease;
}

.floating-theme-toggle:hover i {
  transform: rotate(15deg) scale(1.1);
}

