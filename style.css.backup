            min-height: 320px;
        }
        .skill-card:hover {
            transform: translateY(-6px) scale(1.04); /* Subtler scale on skill hover */
            box-shadow: 0 16px 48px 0 rgba(106,130,251,0.22);
        }
        .skill-card .skill-details-btn {
            margin-top: auto;
        }
        .skill-card .skill-details-spacer {
            flex-grow: 1;
            width: 100%;
        }
        .skill-title {
            font-size: 1.25rem; /* Larger skill title (text-lg) */
