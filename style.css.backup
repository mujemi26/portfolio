        .contact-section {
            text-align: center;
            padding: 0 1rem;
             max-width: 1000px; /* Added max-width to contact section */
            margin-left: auto;
            margin-right: auto;
        }
        .contact-text {
            font-size: 1.2rem; /* Slightly larger contact text (text-lg) */
            color: #e5e7eb; /* Gray-200 */
