/* Hamburger Lines */
.hamburger-line {
    width: 100%;
    height: 3px;
    background-color: #f3f4f6;
    border-radius: 2px;
    transition: all 0.3s ease;
    transform-origin: center;
}

[data-theme="light"] .hamburger-line {
    background-color: #374151;
}

.mobile-menu-btn:hover .hamburger-line {
    background-color: #ffffff;
