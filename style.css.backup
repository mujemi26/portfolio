/* Safe Touch Improvements */
@media (max-width: 768px) {
  /* Safe button improvements */
  .show-certificate-btn, .skill-details-btn {
    min-height: 44px;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  
  .show-certificate-btn:active, .skill-details-btn:active {
    transform: scale(0.96);
  }
  
  /* Safe link improvements */
  .project-link:active {
    transform: scale(0.98);
  }
  
  /* Safe focus improvements */
  .experience-card:focus-within, .project-card:focus-within, 
  .education-card:focus-within, .skill-card:focus-within, 
  .course-card:focus-within {
    outline: 2px solid var(--accent);
    outline-offset: 2px;
  }
  
  .show-certificate-btn:focus, .skill-details-btn:focus {
    outline: 2px solid var(--accent);
    outline-offset: 2px;
  }
}

/* Safe Performance Optimizations */
@media (max-width: 768px) {
  /* Safe animation optimizations */
  .experience-card, .project-card, .education-card, .skill-card, .course-card {
    will-change: transform;
  }
  
  /* Safe reduced motion support */
  @media (prefers-reduced-motion: reduce) {
    .experience-card, .project-card, .education-card, .skill-card, .course-card {
      transition: none;
    }
  }
  
  /* Safe rendering optimizations */
  .experience-card, .project-card, .education-card, .skill-card, .course-card {
    backface-visibility: hidden;
  }
}

/* Safe Accessibility Improvements */
@media (max-width: 768px) {
  /* Safe color contrast improvements */
  .job-description, .project-description {
    color: var(--text);
  }
  
  /* Safe link improvements */
  .project-link {
    color: var(--accent);
  }
}