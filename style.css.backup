            line-height: 1.9;
            text-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);
            text-align: center;
        }
        .contact-button {
            display: flex;
            align-items: center;
            justify-content: center;
            padding: 0.85rem 2.2rem;
            font-size: 1.08rem;
            font-weight: 600;
            color: #fff;
            background: var(--accent);
            border-radius: 0.7rem;
            border: 2.5px solid transparent;
            background-image: linear-gradient(var(--accent), var(--accent)), linear-gradient(120deg, #3b5bdb, #16a085, #3b5bdb);
            background-origin: border-box;
            background-clip: padding-box, border-box;
            box-shadow: 0 4px 18px rgba(59,91,219,0.13), 0 2px 8px 0 #16a08533;
            cursor: pointer;
            outline: none;
            gap: 0.5rem;
            margin: 2.2rem auto 2.5rem auto;
            width: fit-content;
            position: relative;
            overflow: hidden;
            z-index: 1;
            transition: background 0.25s cubic-bezier(.4,0,.2,1), color 0.25s, box-shadow 0.25s, border 0.25s, transform 0.25s, background-position 0.7s;
            background-size: 100% 100%, 200% 200%;
            background-position: 0 0, 0 50%;
            /* 3D effect */
            transform: perspective(600px) translateZ(0) scale(1);
        }
        .contact-button:hover {
            background: #16a085;
            color: #fff;
            box-shadow: 0 10px 32px 0 #16a08544, 0 0 16px 2px #16a08599, 0 2px 8px 0 #3b5bdb33;
            transform: perspective(600px) translateZ(8px) scale(1.055) rotateX(2deg);
            /* Glow pulse */
            animation: buttonGlowPulse 1.2s infinite alternate;
        }
        @keyframes buttonGlowPulse {
            0% { box-shadow: 0 10px 32px 0 #16a08544, 0 0 16px 2px #16a08599, 0 2px 8px 0 #3b5bdb33; }
            100% { box-shadow: 0 16px 48px 0 #16a08566, 0 0 32px 8px #16a085cc, 0 2px 8px 0 #3b5bdb33; }
        }
        .contact-button i {
            transition: transform 0.35s cubic-bezier(.4,0,.2,1);
        }
        .contact-button:hover i {
            transform: translateY(-3px) scale(1.18) rotate(-12deg);
        }
        /* Ripple effect */
        .contact-button .ripple {
            position: absolute;
            border-radius: 50%;
            transform: scale(0);
            animation: ripple-animate 0.6s linear;
            background: rgba(255,255,255,0.35);
            pointer-events: none;
            z-index: 3;
        }
        @keyframes ripple-animate {
            to {
                transform: scale(2.5);
                opacity: 0;
            }
        }
        /* Success state */
        .contact-button.sent {
            background: #16a085 !important;
            color: #fff !important;
            border-color: #16a085 !important;
            box-shadow: 0 0 0 0 #16a085;
            pointer-events: none;
        }
        .contact-button .sent-message {
            display: flex;
            align-items: center;
            gap: 0.5rem;
            font-size: 1.08rem;
            font-weight: 600;
            color: #fff;
            z-index: 4;
            position: relative;
            animation: sentFadeIn 0.4s;
        }
        @keyframes sentFadeIn {
            from { opacity: 0; transform: translateY(10px); }
            to { opacity: 1; transform: translateY(0); }
        }
        .footer {
            text-align: center;
            padding: 2.5rem 0; /* Increased footer padding */
            margin-top: 4rem; /* Increased footer margin */
            background-color: var(--card-bg);
            color: var(--subtitle);
            font-size: 0.9rem; /* Slightly larger footer text (text-sm) */
            border-top: 1px solid rgba(255, 255, 255,0.08);
            font-weight: 500;
        }
        .footer a {
            color: #6ee7b7; /* Cyan link in footer (teal-300) */
            text-decoration: none;
            transition: color 0.2s ease, transform 0.2s ease;
            padding: 0.25rem;
            border-radius: 0.5rem;
            display: inline-flex;
            align-items: center;
            gap: 0.25rem;
        }
        .footer a:hover {
            color: #14b8a6; /* Darker cyan on hover (teal-500) */
            transform: translateX(2px);
        }

        /* Keyframe Animations */
        @keyframes fadeIn {
            from { opacity: 0; }
            to { opacity: 1; }
        }
        @keyframes slideInLeft {
            from { transform: translateX(-50px); opacity: 0; }
            to { transform: translateX(0); opacity:1; }
        }
        @keyframes slideInRight {
            from { transform: translateX(50px); opacity: 0; }
            to { transform: translateX(0); opacity: 1; }
        }
        @keyframes slideInBottom {
            from { transform: translateY(50px); opacity: 0; }
            to { transform: translateY(0); opacity: 1; }
        }
         @keyframes pulse {
            0% { transform: scale(1); }
            50% { transform: scale(1.1); }
            100% { transform: scale(1); }
        }

        @keyframes fadeInScale {
            from {
                opacity: 0;
                transform: scale(0.8);
            }
            to {
                opacity: 1;
                transform: scale(1);
            }
        }

        /* Animation Classes */
        .fade-in, .slide-in-left, .slide-in-right, .slide-in-bottom, .pulse {
            animation: none !important;
        }

