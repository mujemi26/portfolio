/* Enhanced Theme Toggle */
.theme-toggle-btn {
  display: flex;
  align-items: center;
  gap: 0.5rem;
  background: rgba(255, 255, 255, 0.05);
  color: var(--text);
  border: 1px solid rgba(255, 255, 255, 0.1);
  border-radius: 2rem;
  padding: 0.6rem 1.2rem;
  font-size: 0.9rem;
  cursor: pointer;
  transition: all 0.3s ease;
  backdrop-filter: blur(10px);
  font-weight: 500;
  min-width: 100px;
  justify-content: center;
  position: relative;
  overflow: hidden;
}

.theme-toggle-btn::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.1), transparent);
  transition: left 0.5s ease;
}

.theme-toggle-btn:hover::before {
  left: 100%;
}

.theme-toggle-btn:hover {
  background: var(--accent);
  color: #fff;
  border-color: var(--accent);
  transform: translateY(-2px);
  box-shadow: 0 6px 20px rgba(59, 91, 219, 0.3);
}

.theme-toggle-btn:active {
  transform: translateY(0);
}

.theme-toggle-btn i {
  font-size: 1rem;
  transition: transform 0.3s ease;
}

.theme-toggle-btn:hover i {
  transform: rotate(15deg) scale(1.1);
}

.theme-toggle-indicator {
  font-size: 0.85rem;
  font-weight: 600;
  text-transform: capitalize;
  transition: all 0.3s ease;
}

/* Enhanced Hamburger Button */
.hamburger-icon {
            display: flex;
            flex-direction: column;
  gap: 4px;
  padding: 0.5rem;
  border-radius: 0.5rem;
  transition: all 0.3s ease;
}

.hamburger-icon:hover {
  background: rgba(255, 255, 255, 0.1);
}

.hamburger-icon .bar {
  width: 24px;
  height: 2px;
  background: var(--text);
  border-radius: 2px;
  transition: all 0.3s ease;
  transform-origin: center;
}

#hamburger-btn:hover .hamburger-icon .bar {
  background: var(--accent);
}

#hamburger-btn.is-active .hamburger-icon .bar:nth-child(1) {
  transform: translateY(6px) rotate(45deg);
}

#hamburger-btn.is-active .hamburger-icon .bar:nth-child(2) {
  opacity: 0;
}

#hamburger-btn.is-active .hamburger-icon .bar:nth-child(3) {
  transform: translateY(-6px) rotate(-45deg);
}

